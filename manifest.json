{
  "manifest_version": 2,
  "name": "Download Contacts",
  "version": "1.0",
  "description": "Download contacts as CSV file.",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  }
}
